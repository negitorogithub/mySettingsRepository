<keymap version="1" name="Windows copy" parent="$default">
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="Replace">
    <keyboard-shortcut first-keystroke="shift ctrl r" second-keystroke="shift ctrl r" />
  </action>
  <action id="ReplaceInPath">
    <keyboard-shortcut first-keystroke="shift ctrl r" second-keystroke="shift ctrl p" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="ctrl r" />
  </action>
  <action id="SaveAll" />
</keymap>